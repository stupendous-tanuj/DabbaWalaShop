<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Owner_Contact_Number"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <EditText
            android:id="@+id/et_profile_owner_contact"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:background="@drawable/edittext_background"
            android:hint="@string/hint_Owner_Contact_Number"
            android:inputType="phone"
            android:padding="@dimen/dp_10"
            android:maxLength="10"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Support_Contact_Number"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <EditText
            android:id="@+id/et_profile_support_contact"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:background="@drawable/edittext_background"
            android:hint="@string/hint_Support_Contact_Number"
            android:inputType="phone"
            android:padding="@dimen/dp_10"
            android:maxLength="10"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Order_Processing_Contact_Number"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <EditText
            android:id="@+id/et_profile_order_processing_contact"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:background="@drawable/edittext_background"
            android:hint="@string/hint_Order_Processing_Contact_Number"
            android:inputType="phone"
            android:padding="@dimen/dp_10"
            android:maxLength="10"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Email_Id"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <EditText
            android:id="@+id/et_profile_email_id"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:background="@drawable/edittext_background"
            android:hint="@string/hint_Email_Id"
            android:inputType="textEmailAddress"
            android:padding="@dimen/dp_10"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Minimum_Accepted_Order"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <EditText
            android:id="@+id/et_profile_minimum_acc_order"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:background="@drawable/edittext_background"
            android:hint="@string/hint_Minimum_Accepted_Order"
            android:inputType="number"
            android:padding="@dimen/dp_10"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Delivery_Charges"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <EditText
            android:id="@+id/et_profile_delivery_charge"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:background="@drawable/edittext_background"
            android:hint="@string/hint_Delivery_Charges"
            android:inputType="number"
            android:padding="@dimen/dp_10"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Owner_Id_Type"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <TextView
            android:id="@+id/tv_profile_owner_id_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text=""
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Owner_Id_Number"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <TextView
            android:id="@+id/tv_profile_owner_id_number"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text=""
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Associated_To"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <TextView
            android:id="@+id/tv_profile_ssocite_to"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:text=""
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Delivery_Type"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <Spinner
            android:id="@+id/spinner_profile_delivery_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Payment_Method"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13"></TextView>

        <Spinner
            android:id="@+id/spinner_profile_payment_method"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:weightSum="2">

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:layout_weight="0.7"
            android:text="@string/label_Shop_Rating"
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />

        <EditText
            android:id="@+id/et_profile_shop_rating"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.3"
            android:background="@color/white"
            android:enabled="false"
            android:text=""
            android:textColor="@color/black"
            android:textSize="@dimen/sp_13" />
    </LinearLayout>

</LinearLayout>

